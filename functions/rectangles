function makeLine(size) {
    let line = '';
    for (let i = 0; i < size; i++) {
      line += '#';
    }
    return line;
  }

  function makeSquare(size) {
    let square = '';
    for ( let i = 0; i < size; i++);{
    let row = '';
    for (let j = 0; j < size; j++){
      row += '#';
    }
    square += row + '\n';
  }
 retuern square;
}
